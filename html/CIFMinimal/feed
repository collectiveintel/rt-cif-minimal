<%INIT>
require RT::CIFMinimal;
use MIME::Base64;
use Compress::Snappy;
use Data::Dumper;

my $array = RT::CIFMinimal::ReportsByType($session{'CurrentUser'});
$array = [ map { encode_base64(compress($_->encode())) } @$array ];
$array = join("\n",@$array);
$array = 'application/base64+snappy+pb+iodef'."\n".$array;

$r->content_type('text/plain');
$m->out($array);

$m->abort();
</%INIT>
<%args>
</%args>
